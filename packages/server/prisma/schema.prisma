// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// MODEL DEPARTAMENTO
model Departament {
  id String @id @default(uuid())
  dep_name String
  dep_email String
  dep_password String
}